<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>



	<extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.modelexecution.xmof.examples.deployer.gemoc_language_workbench.examples.category"
            name="xMOF Language Examples"
            parentCategory="org.eclipse.ui.Examples"/>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.modelexecution.xmof.examples.deployer.gemoc_language_workbench.examples.category"
            class="org.modelexecution.xmof.examples.deployer.wizards.FSMLanguageExampleWizard"
            finalPerspective="org.eclipse.gemoc.xdsmlframework.ide.ui.xdsml.perspective"
            hasPages="false"
            icon="icons/xmof-moliz-16.png"
            id="org.modelexecution.xmof.examples.deployer.gemoc_language_workbench.examples.deployer.fsm.wizard"
            name="xMOF FSM Language"
            preferredPerspectives="org.eclipse.gemoc.xdsmlframework.ide.ui.xdsml.perspective">
         <description>
            T⁮his set of projects features a simple Finite State Machine (FSM) language which is extended and made executable using GEMOC. For defining the execution semantics of the language, xMOF is used.
         </description>
      </wizard>      
            
   </extension>
   <extension
       point="org.eclipse.help.toc">
    <toc
          file="toc.xml"
          primary="false">
    </toc>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.modelexecution.xmof.examples.deployer.gemoc_modeling_workbench.examples.category"
            name="xMOF Model Examples"
            parentCategory="org.eclipse.ui.Examples">
      </category>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.modelexecution.xmof.examples.deployer.gemoc_modeling_workbench.examples.category"
            class="org.modelexecution.xmof.examples.deployer.wizards.FSMModelExampleWizard"
            finalPerspective="org.eclipse.sirius.ui.tools.perspective.modeling"
            icon="icons/xmof-moliz-16.png"
            id="org.modelexecution.xmof.examples..deployer.wizard1"
            name="Model Example for xMOF FSM Language"
            preferredPerspectives="org.eclipse.sirius.ui.tools.perspective.modeling">
         <description>
            T⁮his project contains some FSM models that can be executed if you have installed the xMOF FSM language.
         </description>
      </wizard>
   </extension>
</plugin>
